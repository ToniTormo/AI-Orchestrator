# API
API_HOST=localhost
API_PORT=8000

# Mocks
USE_MOCKS=False

# GitLab
GITLAB_API_URL=https://gitlab.com/api/v4
GITLAB_API_TOKEN=your-token - Go to Settings > Developer settings > Personal access tokens, click "Generate new token", choose scopes, and copy the token.

# GitHub
GITHUB_API_URL=https://api.github.com
GITHUB_API_TOKEN=your-token - Go to User Settings > Access Tokens, name the token, select scopes, click "Create personal access token", and copy it

# OpenAI (Required)
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-5-mini
OPENAI_TEMPERATURE_ANALYSIS=0.2
OPENAI_TEMPERATURE_DEVELOPMENT=0.3

# Git (Required)
GIT_USERNAME=your-username
GIT_EMAIL=your-email

# Logging Configuration
LOG_LEVEL=INFO
# 5MB
LOG_MAX_BYTES=5242880
LOG_BACKUP_COUNT=5
LOG_MODULE_LEVELS={"main":"INFO","orchestrator":"INFO","evaluator":"INFO","agents":"DEBUG","openai":"INFO","repository":"INFO","testing":"DEBUG","cli":"INFO"}

# Agent
AGENT_TIMEOUT=120
MAX_RETRIES=2

# Notification
NOTIFICATION_EMAIL=your-email
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email
SMTP_PASSWORD=App-Password-at: https://myaccount.google.com/apppasswords
